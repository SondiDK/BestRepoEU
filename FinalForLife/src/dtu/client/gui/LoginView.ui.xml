<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui">
	<ui:style>
	
	</ui:style>
	<g:VerticalPanel width="100%" height="100%"
		verticalAlignment="ALIGN_MIDDLE" horizontalAlignment="ALIGN_CENTER">
		<g:VerticalPanel styleName="cardbox center" spacing="15"
			width="30%" height="400px" horizontalAlignment="ALIGN_CENTER">
			<g:TextBox styleName="style.usernameTextBox" ui:field="usernameTxtBox"
				alignment="CENTER" />
			<g:PasswordTextBox styleName="white-txt input"
				ui:field="passwordTxtBox" alignment="CENTER" />
			<g:Button styleName="button" width="100%" text="Login"
				ui:field="loginBtn" />
		</g:VerticalPanel>
	</g:VerticalPanel>
</ui:UiBinder> 